{
  "name": "coding-template",
  "version": "1.0.0",
  "description": "coding template for html and scss",
  "main": "index.js",
  "scripts": {
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "scss": "node-sass -w --include-path scss scss/ -o css/ --output-style compressed",
    "prefix": "postcss --use autoprefixer --no-map css/ -d css/",
    "build": "webpack --mode production",
    "dev": "webpack -w --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoshi1125hisa/coding-template.git"
  },
  "author": "yoshi1125hisa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoshi1125hisa/coding-template/issues"
  },
  "homepage": "https://github.com/yoshi1125hisa/coding-template#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "ress": "^1.2.2"
  },
  "devDependencies": {
    "cz-customizable": "6.2.0",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.8",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.5",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
