{
  "name": "coding-template",
  "version": "1.0.0",
  "description": "coding template for html and scss",
  "main": "index.js",
  "scripts": {
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "scss": "node-sass -w --include-path scss scss/ -o css/ --output-style compressed",
    "prefix": "postcss --use autoprefixer --no-map css/ -d css/",
    "build": "webpack --mode production",
    "dev": "webpack -w --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoshi1125hisa/coding-template.git"
  },
  "author": "yoshi1125hisa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoshi1125hisa/coding-template/issues"
  },
  "homepage": "https://github.com/yoshi1125hisa/coding-template#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "ress": "^1.2.2"
  },
  "devDependencies": {
    "cz-customizable": "6.2.0",
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.2",
    "file-loader": "3.0.1",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.3.1",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7"
  }
}
